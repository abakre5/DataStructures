#ifndef NODE_H_
#define NODE_H_

#include <bits/stdc++.h>
using namespace std;

class node{
public:
	int data;
	node *next;
	node *setData(int data);
	void setNext(node *temp);
	node *goToNext();
	int getData();
	node* getNext();
};

node *node::setData(int data){
	this->data = data;
	this->next = NULL;
	return this;
}

void node::setNext(node *temp){
	this->next = temp;
}

node *node::goToNext(){
	return this->next;
}

int node::getData(){
	return this->data;
}

node* node::getNext(){
	return this->next;
}

#endif /* NODE_H_ */
