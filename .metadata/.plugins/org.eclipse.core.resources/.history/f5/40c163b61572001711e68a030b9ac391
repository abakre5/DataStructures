#include "LinkedList.h"

int main() {
	LinkedList l;
	l.createlist();
	l.printList();
	l.detectLoop();
	l.printReverseList(l.getHead());
	l.reverseList();
	l.printList();
	l.printMiddle();
	l.deleteFromLinkedList(rand() % 10001);
	return 0;
}
