#ifndef NODE_H_
#define NODE_H_

#include <bits/stdc++.h>
using namespace std;

class node{
	int data;
	node *lchild;
	node *rchild;
public:
	node *setData(int data);
	void setNext(node *temp);
	int getData();
	node* getNext();
};

node *node::setData(int data){
	this->data = data;
	this->next = NULL;
	return this;
}

void node::setNext(node *temp){
	this->next = temp;
}

int node::getData(){
	return this->data;
}

node* node::getNext(){
	return this->next;
}

int generateRandom(){
	return (rand() % 10001);
}

#endif /* NODE_H_ */
