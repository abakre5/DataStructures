#include "trees.h"

int main() {
	Trees t;
	t.createTree();
	t.inOrder(t.getRoot());
	cout << "\nHeight Of Tree :: " << t.heightOfTree(t.getRoot()) << endl;
	cout << "\nHeight Of Tree :: " << t.sizeOfTree(t.getRoot()) << endl;
	return 0;
}
