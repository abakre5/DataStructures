#ifndef NODE_H_
#define NODE_H_

#include <bits/stdc++.h>
using namespace std;

class node{
	int data;
	node *next;
public:
	node* setData(int data);
	int getData();
	node* getNext();
};

node* node::setData(int data){
	this->data = data;
	this->next = NULL;
	return this;
}

int node::getData(){
	return this->data;
}

node* node::getNext(){
	return this->next;
}

#endif /* NODE_H_ */
